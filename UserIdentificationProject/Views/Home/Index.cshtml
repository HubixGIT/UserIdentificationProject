@{
    ViewData["Title"] = "Home Page";
}
<html>
<body>

    <form>
        <div class="form-group">
            <label for="UserLogin">Login</label>
            <input id="UserLogin" type="text" class="form-control" placeholder="ExampleLogin">
        </div>
        <div class="form-group">
            <label for="TextArea">Write some text 150 character limit</label>
            <textarea class="form-control" id="TextArea" rows="3" maxlength="150" onkeyup="countChar(this), processKeyUp(event)" onkeydown="processKeyDown(event)" placeholder="Type Here"></textarea>
        </div>

        <div class="form-group">
            <div class="numbersofChar">150</div>
        </div>
        <div class="form-group">
            <button type="button" class="btn btn-success float-right">Submit</button>
        </div>
        <div id="time" class="form-group">...</div>
        <div id="counter" class="form-group">...</div>
        <table id="myTable">
            <thead>Keydown/Time</thead>
        </table>
        <br>
     
    </form>

    <script src="~/js/TextFunctions.js"></script>
    <link rel="stylesheet" href="~/css/StyleSheet.css" />

</body>
</html>